Protocol CR:

Declarations:
    aenc/2;
    h/1;

Types:
    Agent 		C, R;
    Number 		n;
    Function 	h;
    Mapping 	pk;

Knowledge:
    C: C,R,pk(C),sk(C),pk(R);
    R: C,R,pk(R),sk(R),pk(C);

Public:
    pk(C), pk(R);

Private:
    sk(C), sk(C);

Actions:
    [cr1] C -> R (n) : aenc{n}pk(R);
    [cr2] R -> C     : h(n);

Goals:
    /* Goals that hold */
    n secret between C, R;
    h(n) secret between C,R;
    R ->* C : h(n);


ChannelKeys:

end
