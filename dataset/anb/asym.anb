Protocol Asym:

Declarations:
    aenc/2;

Types:
    Agent A,B;
    Number n;
    Mapping pk

Knowledge:
    A: A, pk(A), sk(A);
    B : B, pk(B),sk(B),pk(A);

Public:
    A,B, pk(B), pk(A);
Private:
    sk(A), sk(B);

Actions:
    [a1] A -> B (n) : aenc{ n }sk(A);

Goals:
    B -> A: n;

ChannelKeys:

end