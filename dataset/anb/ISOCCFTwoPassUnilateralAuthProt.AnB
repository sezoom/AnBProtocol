Protocol ISO_twopass_CCF:

Declarations:
    mac/2;

Types:
    Agent A,B;
    Number NB,Text2,tagISOTCCF1,tagISOTCCF2,tagISOTCCF3;
    Mapping sk;
    Function f;

Knowledge:
    A: A, B, sk(A,B), f;
    B: B, A, sk(A,B), f;

Public:
    A,B;
Private:
    skAB;
Actions:
    [m1] B -> A (NB) : tagISOTCCF1 . NB;
    [m2] A -> B      : mac{ NB. B. Text2}skAB . NB . B . Text2;

Goals:
A *-> B: Text2

ChannelKeys:

end
