Protocol two:

Declarations:
    xor/2;
    senc/2;

Types:
    Agent A,B;
    Number N, M;

Knowledge:
    A : A, B, Ks;
    B : A, B, Ks;


Public:
    A, B;
Private:
    ks;

Actions:
    [m1] A -> B (N) : N;
    [m2] B -> A     : xor(N, Ks);
    [m3] A -> B (M) : senc{xor(M, N)}Ks;

Goals:
    M secret between A,B;

ChannelKeys:

end