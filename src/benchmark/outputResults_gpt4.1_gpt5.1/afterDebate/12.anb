Protocol Twelve:

Declarations:
    senc/2;
    h/1;

Types:
    Agent A,B,C;
    Number N1,N2;
    Symmetric_key Ka,Kb,Kf;
    Function h;

Knowledge:
    A : A, B, C, Ka, Kb;
    B : A, B, C, Ka, Kb;
    C : A, B, C, Ka, Kb;

Public:

Private:
    Ka, Kb;

Actions:
    [t1] A -> C          : A . B;
    [t2] C -> A (Kf)     : senc{ Kf }Ka . senc{ Kf }Kb;
    [t3] A -> B          : senc{ Kf }Kb;
    [t4] B -> A (N2)     : senc{ N2 }Kf;
    [t5] A -> B (N1)     : senc{ N1 }Kf;
    [t6] B -> A          : senc{ h(N1) }Kf;

Goals:
    B ->* A : Kf . N1;
    N1 secret between A,B;
    N2 secret between A,B;
    Kf secret between A,B,C;

ChannelKeys:
    K(A,B): Kf;
    K(B,A): Kf;

end