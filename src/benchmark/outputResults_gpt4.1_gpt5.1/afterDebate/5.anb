Protocol Five:

Declarations:
    senc/2;
    h/1;

Types:
    Agent A,B;
    Number n,m;
    Symmetric_key k;
    Function h;
    Mapping k

Knowledge:
    A : A, B, k(A,B);
    B : A, B, k(A,B);

Public:

Private:
    k(A,B);

Actions:
    [step1] A -> B (n) : senc{ A . B . n . h(n) }k(A,B);
    [step2] B -> A (m) : senc{ m }k(A,B);
    [step3] A -> B     : senc{ h(m) }k(A,B);

Goals:
    B -> A : n;
    A -> B : m;
    n secret between A,B;
    m secret between A,B;

ChannelKeys:

end