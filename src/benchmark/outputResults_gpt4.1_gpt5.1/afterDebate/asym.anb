Protocol Asym:

Declarations:
    aenc/2;
    pk/1;
    sk/1;

Types:
    Agent A,B;
    Number n;
    Mapping pk,sk;

Knowledge:
    A : pk(A), sk(A), pk(B), B;
    B : pk(B), sk(B), pk(A), A;

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [a1] A -> B (n) : aenc{ n }sk(A);
    [a2] B -> A     : n;

Goals:
    B -> A : n;

ChannelKeys:

end