Protocol ASW:

Declarations:
    aenc/2;
    h/1;
    pk/1;
    sk/1;

Types:
    Agent A,B;
    Number n1,n2,m;
    Function h;
    Mapping pk,sk

Knowledge:
    A : pk(A), sk(A), pk(B), B, m;
    B : pk(B), sk(B), pk(A), A, m;

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [asw1] A -> B (n1) : aenc{ pk(A) . pk(B) . m . h(n1) }sk(A);
    [asw2] B -> A (n2) : aenc{ aenc{ pk(A) . pk(B) . m . h(n1) }sk(A) . h(n2) }sk(B);
    [asw3] A -> B      : n1;
    [asw4] B -> A      : n2;

Goals:
    A -> B : m;
    B ->* A : m;

ChannelKeys:

end