Protocol ITMS3:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NA,NB;
    Function senc;
    Mapping k

Knowledge:
    A : A, B, k(A,B);
    B : A, B, k(A,B);

Public:

Private:
    k(A,B);

Actions:
    [m1] B -> A (NB) : B . tagITMS1 . NB;
    [m2] A -> B (NA) : senc{ tagITMS3 . NA . NB . B . Text2 }k(A,B);
    [m3] B -> A      : senc{ tagITMS2 . NB . NA . Text4 }k(A,B);

Goals:
    /* B authenticates A on (NA, NB, Text2) */
    B -> A : (NA, NB, Text2);
    /* A authenticates B on (NB, NA, Text4) */
    A -> B : (NB, NA, Text4);
    /* Secrecy of Text2 and Text4 */
    Text2 secret between A,B;
    Text4 secret between A,B;

ChannelKeys:

end