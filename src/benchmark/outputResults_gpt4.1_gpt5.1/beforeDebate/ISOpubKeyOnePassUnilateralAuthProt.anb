Protocol ISO9798-4-2pass-MAC:

Declarations:
    mac/2;

Types:
    Agent A,B;
    Number NB,Text2,tag9798;
    Symmetric_key sk;
    Function mac;
    Mapping sk

Knowledge:
    A : A, B, sk(A,B), B;
    B : A, B, sk(A,B), A;

Public:
    tag9798;

Private:
    sk(A,B);

Actions:
    [m1] B -> A (NB) : tag9798 . B . NB;
    [m2] A -> B      : NB . B . Text2 . mac{ NB . B . Text2 . tag9798 }sk(A,B);

Goals:
    /* B authenticates A on (NB, B, Text2, tag9798) */
    B -> A : NB . B . Text2 . tag9798;
    Text2 secret between A,B;

ChannelKeys:

end