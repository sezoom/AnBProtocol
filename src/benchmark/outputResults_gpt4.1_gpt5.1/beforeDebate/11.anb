Protocol Eleven:

Declarations:
    aenc/2;
    senc/2;
    h/1;
    g/0;
    exp/2;
    kdf/1;
    sign/2;

Types:
    Agent A,B;
    Number na,nb,N;
    Function h,kdf;
    Mapping pk,sk

Knowledge:
    A : pk(A), sk(A), pk(B), B;
    B : pk(B), sk(B), pk(A), A;

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [e1] A -> B (na) :
        sign{ A . pk(A) . exp(g(),na) }sk(A) . pk(A) . exp(g(),na);

    [e2] B -> A (nb,N) :
        aenc{ N . B . pk(B) . exp(g(),nb) }pk(A);

    [e3] A -> B :
        senc{ h(N) }exp(g(),na*nb);

    [e4] B -> A :
        sign{ h(A . B . exp(g(),na) . exp(g(),nb) . exp(g(),na*nb) . N) }sk(B);

Goals:
    /* Mutual authentication on (A, B, exp(g(),na), exp(g(),nb), exp(g(),na*nb), N) */
    B -> A : (A . B . exp(g(),na) . exp(g(),nb) . exp(g(),na*nb) . N);
    A -> B : (A . B . exp(g(),na) . exp(g(),nb) . exp(g(),na*nb) . N);
    /* Secrecy of N and DH secret */
    N secret between A,B;
    exp(g(),na*nb) secret between A,B;

ChannelKeys:
    K(A,B): exp(g(),na*nb);

end