Protocol ITS2Pass:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NB,Text2,tagITS1,tagITS2;
    Symmetric_key sk;
    Mapping sk

Knowledge:
    A : A, B, sk(A,B);
    B : A, B, sk(A,B);

Public:

Private:
    sk(A,B);

Actions:
    [m1] B -> A (NB) : tagITS1 . NB;
    [m2] A -> B (Text2) : senc{ tagITS2 . NB . B . Text2 }sk(A,B);

Goals:
    /* B authenticates A on Text2, bound to NB and B */
    B -> A : Text2;

ChannelKeys:

end