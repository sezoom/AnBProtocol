Protocol Ten:

Declarations:
    aenc/2;
    senc/2;
    h/1;

Types:
    Agent A, B;
    Number ID_A, ID_B;
    Symmetric_key Kf;
    Payload M;
    Mapping pk: Agent -> Public_key;
    Mapping sk: Agent -> Secret_key;

Knowledge:
    A : sk(A), ID_A;
    B : sk(B), ID_B;

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [step1] A -> B (Kf) : aenc{ ID_A . pk(A) . Kf }sk(A);
    [step2] B -> A () : ID_B . pk(B) . h(Kf);
    [step3] A -> B () : aenc{ senc{ M }Kf . ID_A . ID_B }pk(B);

Goals:
    B weakly authenticates A on Kf with non-injective agreement;

ChannelKeys:
    K(A,B): Kf;
end