Protocol ITMSAuth:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NA,NB,tagITMS1,tagITMS2,tag,Text1,Text3;
    Symmetric_key KAB;

Knowledge:
    A : A, B, KAB, tagITMS1, tagITMS2, tag;
    B : A, B, KAB, tagITMS1, tagITMS2, tag;

Public:

Private:
    KAB;

Actions:
    [p1] A -> B (NA) : senc{ tagITMS1 . NA . B . Text1 . tag }KAB;
    [p2] B -> A (NB) : senc{ tagITMS2 . NB . A . Text3 . tag }KAB;

Goals:
    Authenticated(A, B);
    Authenticated(B, A);
    Secret(Text1, A, B);
    Secret(Text3, A, B);

ChannelKeys:
    ;

end