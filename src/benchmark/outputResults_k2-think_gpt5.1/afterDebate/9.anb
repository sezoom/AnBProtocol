Protocol Nine:

Declarations:
    aenc/2;
    senc/2;

Types:
    Agent A,B,S;
    Number M;
    Symmetric_key Kb;
    Mapping pk,sk;

Knowledge:
    A : A, B, S, sk(A), pk(A), pk(B);
    B : B, A, S, sk(B), pk(B);
    S : S, A, B, sk(S), pk(S);

Public:
    pk(A), pk(B), pk(S);

Private:
    sk(A), sk(B), sk(S), Kb;

Actions:
    [step1] A -> S : aenc{ A . pk(A) }sk(A);
    [step2] B -> S (Kb) : B . A . Kb;
    [step3] S -> B : senc{ pk(A) }Kb;
    [step4] B -> A (M) : aenc{ M }pk(A);

Goals:
    B ->* S : pk(A);
ChannelKeys:
end