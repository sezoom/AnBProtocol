Protocol AndrewRPC:

Declarations:
    senc/2;
    succ/1;

Types:
    Agent A,B;
    Number nA,nB,nB2;
    Symmetric_key KAB;
    Function succ;

Knowledge:
    A : KAB, B;
    B : KAB, A;

Public:

Private:
    KAB;

Actions:
    [step1] A -> B (nA) : A . senc{nA}KAB;
    [step2] B -> A (nB) : senc{ succ(nA) . nB }KAB;
    [step3] A -> B      : senc{ succ(nB) }KAB;
    [step4] B -> A (nB2): senc{nB2}KAB;

Goals:
    B -> A : succ(nA);
    A -> B : succ(nB);
    nA secret between A,B;
    nB secret between A,B;
    nB2 secret between A,B;

ChannelKeys:

end