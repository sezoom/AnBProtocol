Protocol Two:

Declarations:
    xor/2;
    senc/2;

Types:
    Agent A,B;
    Number N,M;
    Symmetric_key Ks;
    Function xor;

Knowledge:
    A : A, B, Ks;
    B : A, B, Ks;

Public:

Private:
    Ks;

Actions:
    [step1] A -> B (N) : N;
    [step2] B -> A     : xor(N, Ks);
    [step3] A -> B (M) : senc{xor(M, N)}Ks;

Goals:
    B -> A : Ks;
    A -> B : Ks;
    M secret between A,B;

ChannelKeys:

end