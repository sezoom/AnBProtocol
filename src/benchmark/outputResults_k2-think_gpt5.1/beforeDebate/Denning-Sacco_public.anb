Protocol DenningSacco:

Declarations:
    aenc/2;

Types:
    Agent A,B,S;
    Number tA;
    Symmetric_key kAB;
    Mapping pk, sk;

Knowledge:
    A : A, B, S, pk(A), sk(A), pk(S);
    B : B, A, S, pk(B), sk(B), pk(S);
    S : S, A, B, sk(S), pk(S), pk(A), pk(B);

Public:
    pk(A), pk(B), pk(S);

Private:
    sk(A), sk(B), sk(S);

Actions:
    [step1] A -> S : A . B ;
    [step2] S -> A : aenc{A . pk(A)}sk(S) . aenc{B . pk(B)}sk(S);
    [step3] A -> B (kAB, tA) : aenc{A . pk(A)}sk(S) . aenc{B . pk(B)}sk(S) . aenc{ aenc{kAB . tA}sk(A) }pk(B);

Goals:
    B ->* A : kAB;
    kAB secret between A,B;

end