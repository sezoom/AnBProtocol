Protocol Asym:

Declarations:
    aenc/2;

Types:
    Agent A,B;
    Number n;
    Mapping pk, sk;

Knowledge:
    A : sk(A), pk(A), pk(B), B;
    B : sk(B), pk(B), pk(A), A;

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [step1] A -> B (n) : aenc{ n } sk(A);

Goals:
    B -> A : n;
    n secret between A,B;

ChannelKeys:

end