Protocol Handshake:

Declarations:
    senc/2;
    kdf/2;

Types:
    Agent A,B;
    Number NA, NB, k, ACK;
    Symmetric_key ;
    Function kdf;
    Mapping pk, sk;

Knowledge:
    A : pk(A), sk(A), pk(B), B;
    B : pk(B), sk(B), pk(A), A;

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [step1] A -> B (NA) : NA;
    [step2] B -> A (NB) : NB;
    [step3] A -> B (k) : senc{ACK}k;

end