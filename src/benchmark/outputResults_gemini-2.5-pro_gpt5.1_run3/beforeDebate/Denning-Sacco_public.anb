Protocol DenningSaccoPub:

Declarations:
    aenc/2;

Types:
    Agent A,B,S;
    Number tA, kAB;
    Mapping pk,sk;

Knowledge:
    A : A, B, S, pk(A), sk(A), pk(B), pk(S);
    B : B, A, S, pk(B), sk(B), pk(A), pk(S);
    S : S, A, B, pk(S), sk(S), pk(A), pk(B);

Public:
    A, B, S, pk(A), pk(B), pk(S);

Private:
    sk(A), sk(B), sk(S), kAB;

Actions:
    /* A requests certificates for itself and B from the server S */
    [m1] A -> S : A . B;

    /* S provides signed certificates for A and B */
    [m2] S -> A : aenc{ A . pk(A) }sk(S) . aenc{ B . pk(B) }sk(S);

    /* A forwards certificates to B and sends the session key kAB */
    [m3] A -> B (kAB, tA) : aenc{ A . pk(A) }sk(S) . aenc{ B . pk(B) }sk(S) . aenc{ aenc{ kAB . tA }sk(A) }pk(B);

Goals:
    /* The session key kAB must remain secret between A and B */
    kAB secret between A,B;

    /* B authenticates A on the session key kAB (non-injective) */
    B ->* A : kAB;

ChannelKeys:
    K(A,B): kAB;

end