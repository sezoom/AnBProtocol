Protocol ISO_twopass_symm:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NB;
    Data   tagITS1, tagITS2, Text2;
    Symmetric_key skAB;

Knowledge:
    A : A, B, skAB, Text2;
    B : A, B, skAB;

Public:
    A, B, tagITS1, tagITS2;

Private:
    skAB;

Actions:
    [m1] B -> A (NB) : tagITS1 . NB;
    [m2] A -> B      : senc{ tagITS2 . NB . B . Text2 }skAB;

Goals:
    /* B non-injectively authenticates A on the data Text2 */
    B ->* A : Text2;

ChannelKeys:
    /* No channel keys are established in this protocol */

end