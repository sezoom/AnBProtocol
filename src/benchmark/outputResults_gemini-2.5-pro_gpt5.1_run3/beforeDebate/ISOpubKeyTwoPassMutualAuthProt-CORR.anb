Protocol ISO_twopass_mutual_CORR:

Declarations:
    aenc/2;

Types:
    Agent A,B,S;
    Number NA,NB;
    Data tagisopubkeytwopmauth1, tagisopubkeytwopmauth2, tag, Text1, Text2;
    Mapping pk,sk;

Knowledge:
    A : A, B, S, pk(A), sk(A), pk(B), pk(S), Text1, tag;
    B : A, B, S, pk(B), sk(B), pk(A), pk(S), Text2, tag;
    S : A, B, S, pk(S), sk(S), pk(A), pk(B);

Public:
    A, B, S, pk(A), pk(B), pk(S), Text1, Text2, tag, tagisopubkeytwopmauth1, tagisopubkeytwopmauth2;

Private:
    sk(A), sk(B), sk(S);

Actions:
    [m1] A -> B (NA) : aenc{ A . pk(A) }sk(S) . aenc{ tagisopubkeytwopmauth1 . NA . B . Text1 . tag }sk(A);
    [m2] B -> A (NB) : aenc{ B . pk(B) }sk(S) . aenc{ tagisopubkeytwopmauth2 . NB . A . Text2 }sk(B);

Goals:
    B ->* A : Text1;
    A ->* B : Text2;

ChannelKeys:

end