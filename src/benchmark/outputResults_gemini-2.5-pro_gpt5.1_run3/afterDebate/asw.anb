Protocol ASW:

Declarations:
    aenc/2;
    h/1;

Types:
    Agent A,B;
    Number N1,N2;
    Data M, Sid;
    Function h;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), sk(A), pk(B), M, Sid;
    B : A, B, pk(B), sk(B), pk(A), Sid;

Public:
    A, B, pk(A), pk(B), M, Sid;

Private:
    sk(A), sk(B);

Actions:
    [asw1] A -> B (N1) : aenc{ pk(A) . pk(B) . Sid . M . h(N1) }sk(A);
    [asw2] B -> A (N2) : aenc{ aenc{ pk(A) . pk(B) . Sid . M . h(N1) }sk(A) . h(N2) }sk(B);
    [asw3] A -> B      : N1;
    [asw4] B -> A      : N2;

Goals:
    /* A achieves injective agreement with B on the session data */
    A -> B : Sid . M . N1 . N2;
    /* A and B achieve non-injective agreement on the session data */
    A ->* B : Sid . M . N1 . N2;
    B ->* A : Sid . M . N1 . N2;

ChannelKeys:
    /* No derived channel keys modeled */
end