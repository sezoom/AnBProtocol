Protocol NSPK:

Declarations:
    aenc/2;

Types:
    Agent A,B;
    Number Na,Nb;
    Data   tagNSPK1,tagNSPK2,tagNSPK3;
    Mapping pk/1,sk/1;

Knowledge:
    A : A, B, pk(A), sk(A), pk(B);
    B : B, A, pk(B), sk(B), pk(A);

Public:
    A, B, pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [m1] A -> B (Na) : aenc{ tagNSPK1 . Na . A }pk(B);
    [m2] B -> A (Nb) : aenc{ tagNSPK2 . Na . Nb }pk(A);
    [m3] A -> B      : aenc{ tagNSPK3 . Nb }pk(B);

Goals:
    /* A authenticates B on the nonces */
    A -> B : Na . Nb;

    /* B authenticates A on its nonce */
    B -> A : Nb;

    /* Nonces remain secret */
    Na secret between A,B;
    Nb secret between A,B;

ChannelKeys:

end