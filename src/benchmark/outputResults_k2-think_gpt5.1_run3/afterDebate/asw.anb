Protocol ASW:

Declarations:
    aenc/2;
    h/1;

Types:
    Agent A,B;
    Number n_1,n_2;
    Data m;
    Symmetric_key;
    Function h;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), sk(A), pk(B), m;
    B : A, B, pk(B), sk(B), pk(A);

Public:
    A,B,pk(A),pk(B);

Private:
    sk(A), sk(B), m;

Actions:
    [asw1] A -> B (n_1) : aenc{ pk(A) . pk(B) . m . h(n_1) }sk(A);
    [asw2] B -> A (n_2) : aenc{ aenc{ pk(A) . pk(B) . m . h(n_1) }sk(A) . h(n_2) }sk(B);
    [asw3] A -> B        : n_1;
    [asw4] B -> A        : n_2;

Goals:
    A ->* B : m;
    B ->* A : m;
    A -> B : m;

ChannelKeys:

end