Protocol ISO_twopass_symm:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NB;
    Data tagITS1, tagITS2, Text2;
    Symmetric_key KAB;
    Function
    Mapping

Knowledge:
    A : A, B, KAB;
    B : A, B, KAB;

Public:
    A, B, tagITS1, tagITS2;

Private:
    KAB;

Actions:
    [m1] B -> A (NB) : tagITS1 . NB;
    [m2] A -> B      : senc{ tagITS2 . NB . B . Text2 }KAB;

Goals:
    B ->* A : Text2;

ChannelKeys:

end