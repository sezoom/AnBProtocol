Protocol Eleven:

Declarations:
    aenc/2;
    senc/2;
    exp/2;
    mul/2;
    h/1;
    g/0;

Types:
    Agent A,B;
    Number na, nb, N;
    Symmetric_key KAB;
    Function exp, mul, h;
    Mapping pk, sk;

Knowledge:
    A : A, B, sk(A), pk(A), pk(B), g(), KAB;
    B : A, B, sk(B), pk(B), pk(A), g(), KAB;

Public:
    A, B, pk(A), pk(B), g();

Private:
    sk(A), sk(B), KAB;

Actions:
    [m1] A -> B (na) : aenc{ exp(g(), na) . A . pk(A) }sk(A) . pk(A);
    [m2] B -> A (nb, N) : aenc{ N . exp(g(), nb) . B . pk(B) }pk(A);
    [m3] A -> B : senc{ h(N) }KAB;
    [m4] B -> A : aenc{ exp(g(), mul(na, nb)) }sk(B);

Goals:
    B *->* A : A . B . exp(g(), na) . exp(g(), nb) . exp(g(), mul(na, nb)) . N;
    N secret between A,B;
    exp(g(), mul(na, nb)) secret between A,B;

ChannelKeys:
    K(A,B): exp(g(), mul(na, nb));
    K(B,A): exp(g(), mul(na, nb));

end