Protocol Asym:

Declarations:
    aenc/2;

Types:
    Agent A,B;
    Number n;
    Data ;
    Symmetric_key ;
    Function ;
    Mapping pk,sk;

Knowledge:
    A : A, pk(A), sk(A), pk(B), B;
    B : B, pk(B), sk(B), pk(A), A;

Public:
    A,B,pk(A),pk(B);

Private:
    sk(A), sk(B);

Actions:
    [m1] A -> B (n) : aenc{ n }sk(A);

Goals:
    B -> A : n;

ChannelKeys:

end