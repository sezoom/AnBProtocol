Protocol Two:

Declarations:
    senc/2;
    xor/2;

Types:
    Agent A,B;
    Number N;
    Data M;
    Symmetric_key Ks;
    Function xor;

Knowledge:
    A : A, B, Ks, M;
    B : A, B, Ks;
Public:
    A, B;
Private:
    Ks;

Actions:
    [t1] A -> B (N) : N;
    [t2] B -> A     : xor(N, Ks);   // N here is the one received in [t1]
    [t3] A -> B     : senc{ xor(M, N) }Ks;

Goals:
    B -> A : N;
    A -> B : N;
    M secret between A,B;
ChannelKeys:
end