Protocol TLS:

Declarations:
    aenc/2;
    senc/2;
    h/1;
    prf/3;

Types:
    Agent A,B,S;
    Number NA,NB,PMS,Sid,PA,PB;
    Data tagTLS1,tagTLS2,tagTLS3,tagTLS4;
    Symmetric_key KAB, KBA;
    Function h,prf;
    Mapping pk,sk;

Knowledge:
    A : A, B, S, pk(A), sk(A), pk(B), pk(S), PA, Sid;
    B : A, B, S, pk(B), sk(B), pk(A), pk(S), PB, Sid;
    S : S, sk(S), pk(S), A, B, pk(A), pk(B);

Public:
    A, B, S, pk(A), pk(B), pk(S);

Private:
    sk(A), sk(B), sk(S), KAB, KBA;

Actions:
    [m1] A -> B (NA) : A . B . Sid . NA . PA . tagTLS1;
    [m2] B -> A (NB) : NB . PB . Sid . NA . tagTLS2 . aenc{ B . pk(B) }sk(S);
    [m3] A -> B (PMS) : aenc{ A . pk(A) }sk(S) . aenc{ PMS }pk(B) . aenc{ h(NB . B . PMS . tagTLS3) }sk(A) . senc{ h(A . B . Sid . NA . NB . PA . PB) }KAB;
    [m4] B -> A      : senc{ h(A . B . NA . NB . Sid . PA . PB . PMS . tagTLS4) }KBA;

Goals:
    B -> A : prf(PMS,NA,NB);
    prf(PMS,NA,NB) secret between A,B;

ChannelKeys:
    KAB: prf(PMS,NA,NB);
    KBA: prf(PMS,NA,NB);

end