Protocol NonReversible:

Declarations:
    senc/2;
    f/2;

Types:
    Agent A,B;
    Number NA,NB;
    Symmetric_key skAB,K;
    Data tagNonReversible1,tagNonReversible2,tagNonReversible3,tagNonReversible4,tagNonReversible5,tagNonReversible6,tagNonReversible7;
    Function f;

Knowledge:
    A : A, B, skAB;
    B : A, B, skAB;

Public:
    A, B, f, tagNonReversible1, tagNonReversible2, tagNonReversible3, tagNonReversible4, tagNonReversible5, tagNonReversible6, tagNonReversible7;

Private:
    skAB;

Actions:
    [m1] B -> A (NB) :
        tagNonReversible1 . B . NB;
    [m2] A -> B (NA,K) :
        tagNonReversible2 . A . senc{ tagNonReversible3 . f(tagNonReversible6, NB) . NA . A . K }skAB;
    [m3] B -> A :
        tagNonReversible4 . B . senc{ tagNonReversible5 . f(tagNonReversible7, NA) }K;

Goals:
    /* A authenticates B on NA (non-injective) */
    B ->* A : NA;
    /* B authenticates A on NB (non-injective, via f(tagNonReversible6, NB)) */
    A ->* B : f(tagNonReversible6, NB);
    /* K is secret between A and B */
    K secret between A,B;

end