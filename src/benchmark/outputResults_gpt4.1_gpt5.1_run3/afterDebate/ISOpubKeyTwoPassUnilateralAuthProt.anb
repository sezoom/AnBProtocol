Protocol ISO_TwoPass:

Declarations:
    aenc/2;

Types:
    Agent A,B,s;
    Number RA,RB;
    Data tagISO_twopass_symm1,tagISO_twopass_symm2,Text2;
    Mapping pk,sk;

Knowledge:
    A : A, pk(A), sk(A), s, pk(s), B, Text2;
    B : B, pk(B), s, pk(s), tagISO_twopass_symm1, tagISO_twopass_symm2;
    s : s, sk(s), pk(s);

Public:
    A,B,s,pk(A),pk(B),pk(s),tagISO_twopass_symm1,tagISO_twopass_symm2;

Private:
    sk(A), sk(s);

Actions:
    [m1] B -> A (RB) : tagISO_twopass_symm1 . RB;
    [m2] A -> B (RA) :
        aenc{ tagISO_twopass_symm2 . A . pk(A) }sk(s)
        .
        aenc{ RA . RB . B . Text2 }sk(A);

Goals:
    /* B authenticates A on (RA, RB, B, Text2) */
    B -> A : RA . RB . B . Text2;

ChannelKeys:

end