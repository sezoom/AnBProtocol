Protocol SimpleKDFHandshake:

Declarations:
    senc/2;
    kdf/2;

Types:
    Agent A,B;
    Number NA,NB;
    Data ACK;

Knowledge:
    A : A, B;
    B : A, B;

Public:
    A, B;

Private:

Actions:
    [m1] A -> B (NA)      : NA;
    [m2] B -> A (NB)      : NB;
    [m3] A -> B           : senc{ ACK }K(A,B);

Goals:
    B -> A : NA . NB;
    K(A,B) secret between A,B;
    ACK secret between A,B;

ChannelKeys:
    K(A,B): kdf(NA,NB);

end