Protocol ISO_onepass_pk:

Declarations:
    aenc/2;

Types:
    Agent A,B;
    Number NA;
    Data tagISO_onepass_pk1, tagISO_onepass_pk2, Text1;
    Mapping pk,sk

Knowledge:
    A : A, B, pk(A), sk(A), pk(B);
    B : A, B, pk(A), pk(B), sk(B);

Public:
    A, B, pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [m1] A -> B (NA) : aenc{ tagISO_onepass_pk1 . A . pk(A) }sk(A) . aenc{ tagISO_onepass_pk2 . NA . B . Text1 }sk(A);

Goals:
    B ->* A : Text1;

ChannelKeys:

end