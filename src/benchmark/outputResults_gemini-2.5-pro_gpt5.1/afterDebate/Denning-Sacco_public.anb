Protocol DenningSacco:

Declarations:
    aenc/2;
    pk/1;
    sk/1;

Types:
    Agent A,B,S;
    Number T;
    Symmetric_key kAB;
    Mapping pk,sk;

Knowledge:
    A : A, B, S, pk(S), pk(A), sk(A);
    B : A, B, S, pk(S), pk(B), sk(B);
    S : A, B, S, pk(S), sk(S), pk(A), pk(B);

Public:
    A, B, S, pk(A), pk(B), pk(S);

Private:
    sk(A), sk(B), sk(S);

Actions:
    [m1] A -> S     : A . B;
    [m2] S -> A (kAB, T) : aenc{ kAB . B . T }pk(A) . aenc{ kAB . A . T }pk(B);
    [m3] A -> B     : aenc{ kAB . A . T }pk(B);

Goals:
    B ->* A : kAB;
    kAB secret between A,B;

ChannelKeys:

end