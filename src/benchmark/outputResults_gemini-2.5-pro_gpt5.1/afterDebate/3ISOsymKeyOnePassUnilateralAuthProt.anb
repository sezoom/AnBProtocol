Protocol ISO_onepass_symm:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number nA,m;
    Mapping k;

Knowledge:
    A : A, B, k(A,B), m;
    B : A, B, k(A,B);

Public:
    A, B;

Private:
    k(A,B);

Actions:
    [step1] A -> B (nA) : senc{ nA . B . m }k(A,B);
    [step2] B -> A      : senc{ nA . m }k(A,B);

Goals:
    m secret between A,B;
    B -> A : nA, m;

ChannelKeys:

end