Protocol STS:

Declarations:
    aenc/2;
    senc/2;
    h/1;
    g/0;

Types:
    Agent A,B;
    Number x,y;
    Function h,g;
    Mapping pk,sk;

Knowledge:
    A : A, B, sk(A), pk(B), g();
    B : A, B, sk(B), pk(A), g();

Public:
    pk(A), pk(B), g();

Private:
    sk(A), sk(B);

Actions:
    [p1] A -> B (x) : g()^x;
    [p2] B -> A (y) : g()^y . senc{ aenc{ h(g()^y . g()^x) }sk(B) }h(g()^(x*y));
    [p3] A -> B     : senc{ aenc{ h(g()^x . g()^y) }sk(A) }h(g()^(x*y));

Goals:
    B -> A : h(g()^(x*y));
    A -> B : h(g()^(x*y));
    h(g()^(x*y)) secret between A,B;

ChannelKeys:
    K(A,B): h(g()^(x*y));
    K(B,A): h(g()^(x*y));

end