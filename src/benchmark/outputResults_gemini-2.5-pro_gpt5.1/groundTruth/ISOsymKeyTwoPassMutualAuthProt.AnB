Protocol ISO_twopass_mutual_symm:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NA,NB,Text1,Text2,Text3,Text4,tag,tagITMS1,tagITMS2;
    Mapping sk

Knowledge:
    A: A, B, sk(A,B), tag;
    B: B, A, sk(A,B), tag;

Public:

Private:

Actions:
    [m1] A -> B (NA) : senc{ tagITMS1 . NA . B . Text1 . tag }sk(A,B);
    [m2] B -> A (NB) : senc{ tagITMS2 . NB . A . Text3 }sk(A,B);

Goals:
    B ->* A : Text3;
    A ->* B : Text1;

ChannelKeys:

end