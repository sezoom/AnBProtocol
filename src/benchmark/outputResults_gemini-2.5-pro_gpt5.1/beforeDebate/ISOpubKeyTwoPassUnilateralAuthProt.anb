Protocol ISO_TwoPass_Unilateral:

Declarations:
    aenc/2;

Types:
    Agent A,B,S;
    Number RA, RB, tagISO_twopass_symm1, tagISO_twopass_symm2, Text2;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), sk(A), pk(S), Text2, aenc{ tagISO_twopass_symm2 . A . pk(A) }sk(S);
    B : B, pk(S);
    S : S, pk(S), sk(S);

Public:
    A, B, S, pk(S), Text2;

Private:
    sk(A), sk(S);

Actions:
    [step1] B -> A (RB) : tagISO_twopass_symm1 . RB;
    [step2] A -> B (RA) : aenc{ tagISO_twopass_symm2 . A . pk(A) }sk(S) . aenc{ RA . RB . B . Text2 }sk(A);

Goals:
    B -> A : RA . RB . B . Text2;

ChannelKeys:

end