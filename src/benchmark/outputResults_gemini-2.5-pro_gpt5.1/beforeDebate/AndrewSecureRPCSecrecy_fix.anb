Protocol AndrewSecureRPC:

Declarations:
    senc/2;
    succ/1;

Types:
    Agent A,B;
    Number nA,nB,nB2;
    Function succ;
    Mapping sk;

Knowledge:
    A : A, B, sk(A,B);
    B : A, B, sk(A,B);

Public:
    A, B;

Private:
    sk(A,B);

Actions:
    [step1] A -> B (nA) : A . senc{ nA }sk(A,B);
    [step2] B -> A (nB) : senc{ succ(nA) . nB }sk(A,B);
    [step3] A -> B      : senc{ succ(nB) }sk(A,B);
    [step4] B -> A (nB2): senc{ nB2 }sk(A,B);

Goals:
    B -> A : nA;
    A -> B : nB;
    nA secret between A,B;
    nB secret between A,B;
    nB2 secret between A,B;

ChannelKeys:

end