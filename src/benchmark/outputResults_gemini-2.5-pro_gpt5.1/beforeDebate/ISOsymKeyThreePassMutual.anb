Protocol ISO9798_2_3pass:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NA,NB,tagB1,tagITMS2,tagITMS3,Text2,Text4;
    Mapping k;

Knowledge:
    A : A, B, k(A,B), Text2;
    B : A, B, k(A,B), Text4;

Public:
    A, B;

Private:
    k(A,B);

Actions:
    [m1] B -> A (NB) : B . tagB1 . NB;
    [m2] A -> B (NA) : senc{ tagITMS3 . NA . NB . B . Text2 }k(A,B);
    [m3] B -> A      : senc{ tagITMS2 . NB . NA . Text4 }k(A,B);

Goals:
    B -> A : NA . Text2;
    A -> B : NB . Text4;
    Text2 secret between A,B;
    Text4 secret between A,B;

ChannelKeys:

end