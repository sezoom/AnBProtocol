Protocol Eight:

Declarations:
    aenc/2;
    h/1;

Types:
    Agent A,B;
    Number M,N;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), sk(A), pk(B), M;
    B : A, B, pk(B), sk(B), pk(A);

Public:
    pk(A), pk(B), M;

Private:
    sk(A), sk(B);

Actions:
    [p1] A -> B     : aenc{ A . M }sk(A);
    [p2] B -> A (N) : B . pk(B) . aenc{ N }sk(B);
    [p3] A -> B     : aenc{ N . pk(A) }pk(B);
    [p4] B -> A     : h(M . B . N);

Goals:
    B ->* A : A . M;
    A ->* B : B . N;

ChannelKeys:

end