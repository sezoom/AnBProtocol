Protocol TwoPassSymm:

Declarations:
   aenc/2;

Types:
    Agent A,B,S;
    Number RA,RB;
    Data Text2, tagISO_twopass_symm1, tagISO_twopass_symm2;
    Mapping pk,sk;


Knowledge:
    A : A, pk(A), sk(A), pk(S);
    B : B, pk(B), pk(S), pk(A);
    S : S, pk(S), sk(S);
Public:
    A, B, pk(S), pk(A), pk(B), tagISO_twopass_symm1, tagISO_twopass_symm2;
Private:
    sk(A), sk(S);

Actions:
    [m1] B -> A (RB) : tagISO_twopass_symm1 . RB;
    [m0] S -> A : aenc{ tagISO_twopass_symm2 . A . pk(A) }sk(S);
    [m2] A -> B (RA) : aenc{ tagISO_twopass_symm2 . A . pk(A) }sk(S) . aenc{ RA . RB . B . Text2 }sk(A);

Goals:
    B -> A : aenc{ RA . RB . B . Text2 }sk(A);
ChannelKeys:

end