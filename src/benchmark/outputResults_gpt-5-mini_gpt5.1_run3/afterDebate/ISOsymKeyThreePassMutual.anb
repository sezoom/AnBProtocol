Protocol ISO9798_3_MutualSymmetric:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NA,NB;
    Data  tagITMS3, tagITMS2, Text2, Text4;
    Symmetric_key KAB;

Knowledge:
    A : A, B, KAB;
    B : A, B, KAB;

Public:
    A, B;

Private:
    KAB;

Actions:
    [m1] B -> A (NB) : B . A . tagITMS3 . NB;
    [m2] A -> B (NA) : NA . senc{ tagITMS3 . NA . NB . B . Text2 }KAB;
    [m3] B -> A      : senc{ tagITMS2 . NB . NA . Text4 }KAB;

Goals:
    /* Verifier B authenticates Claimant A on NA, NB and Text2 (data-origin) */
    B -> A : NA . NB . Text2;
    /* Claimant A authenticates Verifier B on NB, NA and Text4 (data-origin) */
    A -> B : NB . NA . Text4;

ChannelKeys:
    K(A,B): KAB;
    K(B,A): KAB;
end