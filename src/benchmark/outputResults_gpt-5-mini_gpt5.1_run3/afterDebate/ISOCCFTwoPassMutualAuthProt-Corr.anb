Protocol ISO_twopass_CCF:

Declarations:
   mac/2;

Types:
    Agent A,B;
    Number NB;
    Data Text2, tagISOTCCF1;
    Symmetric_key skAB;

Knowledge:
    A : A, B, skAB, Text2, tagISOTCCF1;
    B : A, B, skAB, tagISOTCCF1;
Public:
    A, B, tagISOTCCF1;
Private:
    skAB;

Actions:
    [m1] B -> A (NB) : tagISOTCCF1 . NB;
    [m2] A -> B      : mac( NB . B . Text2, skAB ) . NB . B . Text2;

Goals:
B ->* A : Text2;

ChannelKeys:

end