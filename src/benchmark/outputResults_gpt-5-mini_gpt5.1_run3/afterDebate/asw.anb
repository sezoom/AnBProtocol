Protocol ASW:

Declarations:
   aenc/2;
   h/1;
   pk/1;
   sk/1;

Types:
    Agent A,B;
    Number N1,N2;
    Data m;
    Function h;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), sk(A), pk(B), m;
    B : A, B, pk(B), sk(B), pk(A);

Public:
    A, B, pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [asw1] A -> B (N1) : aenc{ aenc{ pk(A) . pk(B) . m . h(N1) }sk(A) }pk(B);
    [asw2] B -> A (N2) : aenc{ aenc{ aenc{ pk(A) . pk(B) . m . h(N1) }sk(A) . h(N2) }sk(B) }pk(A);
    [asw3] A -> B : aenc{ N1 }pk(B);
    [asw4] B -> A : aenc{ N2 }pk(A);

Goals:
    A ->* B : m;
    B ->* A : m;
    A -> B : m;
    m secret between A,B;

ChannelKeys:

end