Protocol One:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number Na;
    Data M;
    Symmetric_key Ks;

Knowledge:
    A : A, B, Ks, M;
    B : A, B, Ks;

Public:
    A, B;

Private:
    Ks, M;

Actions:
    [step1] A -> B (Na) : Na;
    [step2] B -> A : senc{ Na }Ks;
    [step3] A -> B : senc{ M }Ks;

Goals:
    A -> B : Na;
    M secret between A,B;

ChannelKeys:

end