Protocol ISO9798-AE:

Declarations:
    senc/2;

Types:
    Agent A,B;
    Number NA,NB,tagITMS1,tagITMS2,tag,Text1,Text3;
    Symmetric_key KAB;

Knowledge:
    A : A, B, KAB;
    B : A, B, KAB;

Public:
    tagITMS1, tagITMS2, tag;

Private:
    KAB;

Actions:
    [m1] A -> B (NA) : senc{ tagITMS1 . NA . B . Text1 . tag }KAB;
    [m2] B -> A (NB) : senc{ tagITMS2 . NB . A . Text3 . tag }KAB;

Goals:
    /* Mutual entity authentication (each side proves possession of KAB) */
    B -> A : NA;
    B -> A : Text1;
    A -> B : NB;
    A -> B : Text3;
    /* Confidentiality/integrity of protected application fields and nonces */
    Text1 secret between A,B;
    Text3 secret between A,B;
    NA secret between A,B;
    NB secret between A,B;
    KAB secret between A,B;

end