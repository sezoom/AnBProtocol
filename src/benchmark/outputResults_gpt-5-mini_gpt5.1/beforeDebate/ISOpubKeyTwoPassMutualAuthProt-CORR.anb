Protocol ISO9798_sig_mutual:

Declarations:
    aenc/2;

Types:
    Agent A,B,S;
    Number NA,NB,tagA,tagB,Domain,Text1,Text3;
    Mapping pk,sk

Knowledge:
    A : A, B, pk(A), sk(A), pk(S), pk(B), Text1, aenc{ A . pk(A) }sk(S);
    B : A, B, pk(B), sk(B), pk(S), pk(A), Text3, aenc{ B . pk(B) }sk(S);
    S : S, pk(S), sk(S), A, B;

Public:
    pk(A), pk(B), pk(S), tagA, tagB, Domain;

Private:
    sk(A), sk(B), sk(S);

Actions:
    [step1] A -> B (NA) : aenc{ A . pk(A) }sk(S) . aenc{ tagA . NA . B . Text1 . Domain }sk(A);
    [step2] B -> A (NB) : aenc{ B . pk(B) }sk(S) . aenc{ tagB . NB . A . Text3 . Domain }sk(B);

Goals:
    B -> A : tagA . NA . B . Text1 . Domain;
    A -> B : tagB . NB . A . Text3 . Domain;

ChannelKeys:

end