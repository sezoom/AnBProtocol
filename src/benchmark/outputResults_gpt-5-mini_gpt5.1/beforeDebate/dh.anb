Protocol DH_OneWayConfirm:

Declarations:
    g/0;
    exp/2;
    kdf/2;
    senc/2;

Types:
    Agent A,B;
    Number NX,NY,NA;
    Symmetric_key KAB;
    Function kdf

Knowledge:
    A : A, B, g;
    B : A, B, g;

Public:
    g;

Private:

Actions:
    [step1] A -> B (NX) : exp(g,NX);
    [step2] B -> A (NY) : exp(g,NY);
    [step3] A -> B (NA) : senc{ A . B . NA }kdf(exp(exp(g,NX),NY), A . B);

Goals:
    B -> A : NA;
    exp(exp(g,NX),NY) secret between A,B;

ChannelKeys:
    K(A,B): kdf(exp(exp(g,NX),NY), A . B);
    K(B,A): kdf(exp(exp(g,NX),NY), A . B);

end