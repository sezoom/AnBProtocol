Protocol Five:

Declarations:
    senc/2;
    h/1;
    pk/1;
    sk/1;

Types:
    Agent A,B;
    Number NA,NB;
    Symmetric_key KAB;
    Function h;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), pk(B), KAB;
    B : A, B, pk(A), pk(B), KAB;

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B), KAB;

Actions:
    [p1] A -> B (NA) : senc{ A . B . NA . h(NA) }KAB;
    [p2] B -> A (NB) : senc{ B . A . NB }KAB;
    [p3] A -> B      : senc{ h(NB) }KAB;

Goals:
    A -> B : NB;
    B -> A : h(NB);
    KAB secret between A,B;
    NA secret between A,B;
    NB secret between A,B;

ChannelKeys:

end