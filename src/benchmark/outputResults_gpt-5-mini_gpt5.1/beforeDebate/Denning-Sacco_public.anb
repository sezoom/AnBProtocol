Protocol DenningSacco:

Declarations:
    aenc/2;

Types:
    Agent A,B,S;
    Number tA;
    Symmetric_key KAB;
    Mapping pk,sk

Knowledge:
    A : A, B, pk(S), pk(A), sk(A);
    B : A, B, pk(S), pk(B), sk(B);
    S : A, B, pk(S), sk(S);

Public:
    pk(A), pk(B), pk(S);

Private:
    sk(A), sk(B), sk(S);

Actions:
    [s1] A -> S () : A . B ;
    [s2] S -> A () : aenc{ A . pk(A) }sk(S) . aenc{ B . pk(B) }sk(S) ;
    [s3] A -> B (KAB, tA) : aenc{ A . pk(A) }sk(S) . aenc{ B . pk(B) }sk(S) . aenc{ aenc{ KAB . tA }sk(A) }pk(B) ;

Goals:
    B ->* A : KAB;
    KAB secret between A,B;

ChannelKeys:

end