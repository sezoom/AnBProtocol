Protocol BilateralKE:

Declarations:
    aenc/2;
    senc/2;
    h/1;

Types:
    Agent A,B;
    Number NA,NB;
    Symmetric_key k;
    Function h;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), sk(A), pk(B);
    B : A, B, pk(B), sk(B), pk(A);

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [step1] B -> A (NB) : B . aenc{ NB . B }pk(A);
    [step2] A -> B (NA, k) : aenc{ h(NB) . NA . A . k }pk(B);
    [step3] B -> A      : senc{ h(NA) }k;

Goals:
    A ->* B : k;
    k secret between A,B;

end