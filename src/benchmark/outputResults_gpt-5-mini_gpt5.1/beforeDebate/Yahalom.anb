Protocol Yahalom:

Declarations:
    senc/2;

Types:
    Agent A,B,S;
    Number NA,NB;
    Symmetric_key KAS,KBS,KAB;
    Function ;
    Mapping

Knowledge:
    A : A, B, KAS;
    B : A, B, KBS;
    S : A, B, KAS, KBS;
Public:

Private:
    KAS, KBS;

Actions:
    [step1] A -> B (NA) : A . NA ;
    [step2] B -> S (NB) : B . senc{ A . NA . NB }KBS ;
    [step3] S -> A (KAB) : senc{ B . KAB . NA . NB }KAS . senc{ A . KAB }KBS ;
    [step4] A -> B     : senc{ A . KAB }KBS . senc{ NB }KAB ;

Goals:
    A -> B : KAB;
    B -> A : KAB;
    KAB secret between A,B;

ChannelKeys:
    K(A,B): KAB;
    K(B,A): KAB;

end