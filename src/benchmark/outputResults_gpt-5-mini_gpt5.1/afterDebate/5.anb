Protocol Five:

Declarations:
    senc/2;
    h/1;

Types:
    Agent A,B;
    Number NA,NB;
    Symmetric_key KAB;
    Function h;

Knowledge:
    A : A, B, KAB;
    B : A, B, KAB;
Public:

Private:
    KAB;

Actions:
    [p1] A -> B (NA) : senc{ A . B . NA . h(NA) }KAB;
    [p2] B -> A (NB) : senc{ B . A . NB }KAB;
    [p3] A -> B      : senc{ h(NB) }KAB;

Goals:
    B -> A : NB;
    A -> B : h(NB);
    KAB secret between A,B;
    NA secret between A,B;
    NB secret between A,B;

ChannelKeys:

end