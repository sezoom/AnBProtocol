Protocol Ten:

Declarations:
    aenc/2;
    senc/2;
    h/1;

Types:
    Agent A,B;
    Number M;
    Symmetric_key Kf;
    Function h;
    Mapping pk,sk;

Knowledge:
    A : A, B, pk(A), sk(A), pk(B);
    B : A, B, pk(B), sk(B), pk(A);

Public:
    pk(A), pk(B);

Private:
    sk(A), sk(B);

Actions:
    [step1] A -> B (Kf) : A . pk(A) . Kf . aenc{ A . pk(A) . Kf }sk(A);
    [step2] B -> A () : B . pk(B) . h(Kf);
    [step3] A -> B () : aenc{ senc{ M . A . B }Kf }pk(B);

Goals:
    B ->* A : Kf;
    Kf secret between A,B;

ChannelKeys:

end